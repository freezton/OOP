<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="320.0" prefWidth="389.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.controllers.ReviewController">
   <children>
      <Pane layoutX="14.0" layoutY="14.0" prefHeight="294.0" prefWidth="360.0" style="-fx-background-color: EAEAEA;">
         <children>
            <TextField fx:id="ratingField" layoutX="222.0" layoutY="49.0" prefHeight="30.0" prefWidth="124.0" promptText="Raiting (1 to 5)">
               <font>
                  <Font size="14.0" />
               </font>
            </TextField>
            <ComboBox fx:id="productComboBox" layoutX="14.0" layoutY="49.0" prefHeight="30.0" prefWidth="194.0" />
            <Label layoutX="101.0" layoutY="6.0" text="Review properties">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <TextArea fx:id="commentField" layoutX="14.0" layoutY="91.0" prefHeight="157.0" prefWidth="332.0" promptText="Review comment">
               <font>
                  <Font size="14.0" />
               </font>
            </TextArea>
            <Button layoutX="14.0" layoutY="257.0" mnemonicParsing="false" onAction="#onConfirmButtonClick" prefHeight="30.0" prefWidth="157.0" text="Confirm">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button fx:id="cancelButton" layoutX="189.0" layoutY="257.0" mnemonicParsing="false" onAction="#onCancelButtonClick" prefHeight="30.0" prefWidth="157.0" text="Cancel">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
         </children>
      </Pane>
   </children>
</AnchorPane>
